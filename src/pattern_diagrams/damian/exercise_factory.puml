}

class IndoorExerciseFactory{
    - name : String
    - difficulty : ExerciseDifficulty
    - description : String
    - duration : int
    - caloriesBurned : int
    - category : ExerciseCategory
    + createYogaExercise() : YogaExercise
}

class YogaExercise{

}

OutdoorExerciseFactory ..|> ExerciseFactory
IndoorExerciseFactory ..|> ExerciseFactory
OutdoorExerciseFactory ..> FootballExercise
