interface ExerciseFactory{
    + createFootballExercise() : FootballExercise
    + createYogaExercise() : YogaExercise
}

enum ExerciseCategory{
    SPORT
    PLACE
}

enum ExerciseDifficulty{
    BEGINNER
    INTERMEDIATE
    ADVANCED
    PROFESSIONAL
}

class OutdoorExerciseFactory{
    - name : String
    - difficulty : ExerciseDifficulty
    - description : String
    - duration : int
    - caloriesBurned : int
    - category : ExerciseCategory
    + createFootballExercise() : FootballExercise
}

